{
    "manifest_version": 3,
    "type": "extension",
    "name": "Ai对话",
    "slug": "Byboy_Ai",
    "description": "Ai对话,用于对话,需要自己配置openai的key",
    "version": "0.0.1",
    "thumbnail": "./icon.png",
    "author": {
        "name": "Byboy",
        "link": "https://github.com/ByboyCN"
    },
    "repository": {
        "repo": "ByboyCN/Byboy_Ai",
        "branch": "main",
        "use_release": {
            "tag": "latest",
            "name": "Byboy_Ai.zip"
        }
    },
    "platform": [
        "win32",
        "linux",
        "darwin"
    ],
    "injects": {
        "renderer": "./src/renderer.js",
        "main": "./src/main.js",
        "preload": "./src/preload.js"
    }
}
